{
  "name": "hc-scaffold",
  "version": "0.1.0",
  "description": "Cli and Web wizard for generating scaffolding for holochain applications.",
  "main": "index.js",
  "scripts": {
    "test": "standard --fix && node ./bin/build.js && ninja && node ./bin/exec-tests.js",
    "start": "node ./bin/demo-server.js",
    "start:build": "standard --fix && node ./bin/build.js && ninja && node ./bin/demo-server.js",
    "clean": "ninja -t clean",
    "preversion": "npm test",
    "postversion": "git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holochain/scaffold.git"
  },
  "author": "",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/holochain/scaffold/issues"
  },
  "homepage": "https://github.com/holochain/scaffold#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "browserify": "^16.2.2",
    "chai": "^4.1.2",
    "handlebars-gen": "0.0.11",
    "jsonlint": "^1.6.3",
    "node-ninja": "^1.0.2",
    "phantomjs-prebuilt": "^2.1.16",
    "semver": "^5.5.0",
    "standard": "^11.0.1",
    "uglify-js": "^3.4.2"
  },
  "dependencies": {
    "@holochain/dna-to-openapi": "^0.2.1",
    "ajv": "^5.5.2",
    "babel-polyfill": "^6.26.0",
    "express": "^4.16.3",
    "handlebars": "^4.0.11",
    "jszip": "^3.1.5",
    "opn": "^5.3.0",
    "sprintf-js": "^1.1.1",
    "yaml-js": "^0.2.3"
  },
  "standard": {
    "env": {
      "mocha": true,
      "browser": true
    },
    "ignore": [
      "docs",
      "src/gen"
    ]
  }
}
